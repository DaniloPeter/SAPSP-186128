{
  "_version": "1.58.0",
  "sap.app": {
    "id": "com.segezha.form.roll.conversion",
    "type": "application",
    "title": "Регистрация преобразования рулонов",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "odata": {
        "uri": "/sap/opu/odata/sap/Z_MM211_PREQ_MON_SRV",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "defaultCountMode": "InlineRepeat",
          "useBatch": true
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.segezha.form.roll.conversion.view.App",
      "type": "XML",
      "async": true
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "path": "com.segezha.form.roll.conversion.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "type": "View",
        "viewType": "XML",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "home",
          "target": "home"
        }
      ],
      "targets": {
        "home": {
          "name": "Home"
        }
      }
    },
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.ui.core": {},
        "sap.ui.comp": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "cozy": true,
      "compact": false
    },
    "models": {
      "": {
				"dataSource": "odata",
				"preload": true,
				"settings": {
					"odataVersion": "2.0",
					"defaultCountMode": "InlineRepeat",
					"useBatch": true,
					"headers": {
						"Accept": "application/json"
					}
				}
			},
      "printMode": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/PrintMode.json"
      },
      "store": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/Store.json"
      },
      "shift": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/Shift.json"
      },
      "downTime": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/DownTime.json"
      },
      "crew": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/Crew.json"
      },
      "engineer": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/Engineer.json"
      },
      "defect": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/Defect.json"
      },
      "resource": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/Resource.json"
      },
      
      "state": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "table": {
            "busy": false,
            "items": [
              {
                "downTimeType": 0,
                "dateBegin": null,
                "dateEnd": null,
                "downTime": null,
                "downTimeReason": null
              },
              {
                "downTimeType": 1,
                "dateBegin": null,
                "dateEnd": null,
                "downTime": null,
                "downTimeReason": null
              }
            ]
          },
          "braks": {
            "busy": false,
            "items": [
              {
                "defect": 4,
                "comment": ""
              }
            ]
          }
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    }
  }
}