{
  "_version": "1.58.0",
  "sap.app": {
    "id": "com.segezha.form.roll.conversion",
    "type": "application",
    "title": "{{appTitle}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "odata": {
        "uri": "/sap/opu/odata/sap/ZPP_218_SRV_SRV",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "defaultCountMode": "InlineRepeat",
          "useBatch": true
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.segezha.form.roll.conversion.view.App",
      "type": "XML",
      "async": true
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "path": "com.segezha.form.roll.conversion.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "type": "View",
        "viewType": "XML",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "home",
          "target": "home"
        }
      ],
      "targets": {
        "home": {
          "name": "Home"
        }
      }
    },
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.ui.core": {},
        "sap.ui.comp": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "cozy": true,
      "compact": false
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.segezha.form.roll.conversion.i18n.i18n"
        }
      },
      "": {
        "dataSource": "odata",
        "preload": true,
        "settings": {
          "odataVersion": "2.0",
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "InlineRepeat",
          "useBatch": true,
          "headers": {
            "Accept": "application/json"
          }
        }
      },
      "state": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "switches": {
            "defect": false,
            "downTime": false
          },
          "tables": {
            "defect": {
              "items": []
            },
            "downTime": {
              "items": []
            }
          },
          "errorFields": {}
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    }
  }
}